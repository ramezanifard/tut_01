FloatLayout:
    canvas:
        Color:
            rgba: [0.937, 0.937, 0.937, 0.8]
        Rectangle:
            pos:self.pos
            size: self.size

    

    #Joggint Sectoin
    MDLabel:
        text: "JOGGING" 
        halign: "center"
        font_size: 20
        bold: True
        size_hint: .2, .15
        pos_hint:{"center_x": app.joging_left , "center_y": app.joging_top + .02}
    #Activate button
    MDRaisedButton:
        id: Activate_Jogging
        text: "Activate"
        font_size: 20
        opacity: 1
        size_hint: .2, .075
        pos_hint:{"center_x": app.joging_left , "center_y": app.joging_top - .05}
        on_press: app.activate_joging(*args)
        disabled: False
    #LED
    Image:
        id: led_01_off
        source: "Images/green-led-slim-off.png"
        allow_stretch: False
        size_hint: .03, .03
        pos_hint:{"center_x": app.joging_left + .12, "center_y": app.joging_top - .05}
        opacity: 0.05
    Image:
        id: led_01_on
        source: "Images/green-led-slim-on.png"
        allow_stretch: False
        size_hint: .03, .03
        pos_hint:{"center_x": app.joging_left + .12, "center_y": app.joging_top - .05}
        opacity: 0.95
    MDSlider:
        id: slider_J6
        min: -350
        max: 350
        value: 0
        step:.5
        hint: True
        on_touch_move: app.slider_J6_function(slider_J6.value)
        size_hint: .35, .05
        pos_hint:{"center_x": app.joging_left, "center_y": app.joging_top - .14} 
        disabled: True
    MDLabel:
        text: "J6: %.1f°" % slider_J6.value
        halign: "center"
        size_hint: .25, .05
        pos_hint:{"center_x": app.joging_left + .2, "center_y": app.joging_top - .14}
